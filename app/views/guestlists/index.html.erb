<h1> Your GuestList: </h1>




<table style="width:100%" border=1>
 
<thead>
<tr>
  <th> Guest List ID: </th>
  <th> Guest Name: </th>
  <th> Status: </th>
  <th> Event ID: </th>
</tr>
</thead>



<% @guestlists.each do |guestlist| %> 
<tr>
  <td>
    <%= guestlist.id %>
  </td> 
  <td>
    <%= guestlist.name %>
  </td> 
  <td>
    <a href=""><%= guestlist.status %></a>
  </td> 
  <td>
    <%= guestlist.event_id %> 
  </td> 
</tr>
<% end %>

</table>

