<h1> Your GuestList: </h1>

<% if current_user %>
  <table style="width:100%" border=1>
 
<thead>
<tr>
  <th> Guest List ID: </th>
  <th> Guest Name: </th>
  <th> Status: </th>
  <th> Event ID: </th>
</tr>
</thead>

<% @guestlists.each do |guestlist| %> 
<tr>
  <td>
    <%= guestlist.id %>
  </td> 
  <td>
    <%= guestlist.name %>
  </td> 
  <td>
    <%= guestlist.status %></a>
  </td> 
  <td>
    <%= guestlist.event_id %> 
  </td> 
  <% end %>
</tr>


</table>
<% end %>