<h1> Your Planned Events: </h1>
  <% if current_user %>
  <table class="table">
  <thead class="thead-inverse">
    <tr>
    
<th> Event Name: </th>
<th> Event Info: </th>

    </tr>
    </thead>
  
    <tbody>
    <% @events.each do |event| %>
    <tr>
    
<td> <%= event.event_type.name %> </td>
<td> <%= event.description %> </td>
    </tr>
  <% end %> 
  <% end %>
  </tbody>
  </table>
